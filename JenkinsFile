pipeline{
    agent any

    stages{
        stage('Compile stage'){
            steps{
                    bat 'mvn clean compile'
            
            }
        }
        stage('Testing stage'){
             steps{
                    withMaven(maven : 'Maven 3.5.3'){
                            bat 'mvn test'
                   }
              }
        }
        stage('deploy stage'){
              steps{
                            withMaven(maven : 'Maven 3.5.3'){
                                    bat 'mvn deploy'
                           }
                      }
              }
    }
}
